/* pages/my/my.wxss */
page{
  background: #eee;
}
.header{
  width: 100%;
  height: 260rpx;
}
.dd{
  width: 96%;
  height: 200rpx;
  margin: 0 auto;
  background: #fff;
  border-radius: 20rpx;
}
.myDd{
  font-size: 34rpx;
  padding: 20rpx;
}
.fuwu{
  width: 96%;
  height: 200rpx;
  margin: 0 auto;
  background: #fff;
  margin-top: 20rpx;
  border-radius: 20rpx;
  display: flex;
  flex-wrap: nowrap;
}
.f_item{
  width: 20%;
  height: 200rpx;
  padding: 0 10rpx;
  box-sizing: border-box;
  font-size: 28rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.f_item image{
  width: 80rpx;
  height: 80rpx;
  margin-bottom: 10rpx;
}
.tg{
  width: 96%;
  height: 100rpx;
  margin: 0 auto;
  line-height: 100rpx;
  background: #fff;
  margin-top: 20rpx;
  border-radius: 20rpx;
  padding: 0 20rpx;
  box-sizing: border-box;
}